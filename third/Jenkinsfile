pipeline {
  agent any
 
  tools {nodejs "node_18.7.0"}
 
  stages {
    stage('Download Git Repository') {
      steps {
        git branch: 'main', url: 'https://github.com/bcalvessilva/firstnewmanexample.git'
      }
    }
    stage('Run') {
      steps {
        sh 'newman run AULA_09.postman_collection.json -e AULA_09.postman_environment.json -d AULA_09-users.json'
      }
    }
  }
}
